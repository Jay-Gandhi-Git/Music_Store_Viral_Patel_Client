@model IEnumerable<Music.Video>
@{
    ViewBag.Title = "VideoInfo";
    Layout = "~/Views/Shared/Admin_Layout_Page.cshtml";
    ViewBag.PageTitle = "Video Information";
}
<script>
    $(document).ready(function () {


        $("#menu1").children().removeClass("active");
        $("#menu1").children("#liVideoInfo").addClass("active");

    });
</script>
<div class="container">
    <a href="@Url.Action("VideoCreate","Admin")" class="btn btn-success" style="margin-top:10px;margin-bottom:5px;">Create New Video</a>
    <div class="table-responsive">
        <table class="table table-hover">
            <tr>
                <th>Title</th>
                <th>Create On</th>
                <th>Modified On</th>
                <th>Action</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(model => item.Title)</td>
                    <td>@Html.DisplayFor(model => item.Created_On).ToString()</td>
                    <td>@Html.DisplayFor(model => item.Modified_On).ToString()</td>
                    <td>@Html.ActionLink("Edit", "VideoView", new { id = item.Video_Id }) | @Html.ActionLink("Delete", "VideoDelete", new { id = item.Video_Id }) </td>

                </tr>
            }

        </table>
    </div>
</div>

