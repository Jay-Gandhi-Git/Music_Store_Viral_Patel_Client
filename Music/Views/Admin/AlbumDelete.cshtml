@model Music.PhotoAlbum
@{
    ViewBag.Title = "AlbumDelete";
    Layout = "~/Views/Shared/Admin_Layout_Page.cshtml";
    ViewBag.PageTitle = "Album Dlete";
}

<link href="~/css/lightcase.css" rel="stylesheet" type="text/css" />
<link rel="stylesheet" href="~/css/font-awesome.min.css" />
<link href="//fonts.googleapis.com/css?family=Varela+Round&subset=hebrew" rel="stylesheet">
<link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>

<script>
    $(document).ready(function () {


        $("#menu1").children().removeClass("active");
        $("#menu1").children("#liAlbumInfo").addClass("active");

    });
</script>
@if (ViewBag.error != null)
{
    <div class="container" align="center">
        <span style="color:#FF3720">@ViewBag.error</span>
    </div>
}
<div class="mail">
    <div class="container">
        <div class="col-md-6 agileits_mail_grid_left">
            <div class="table-responsive">
                <form method="post" enctype="multipart/form-data">
                    <h4>Title*</h4>
                    <input type="text" name="title" value="@Html.DisplayFor(model => model.Title)" disabled>
                    <h4>Created On*</h4>
                    <input type="text" value="@Html.DisplayFor(model=>model.Created_On)" disabled>
                    <h4>Modifiedd On*</h4>
                    <input type="text" value="@Html.DisplayFor(model=>model.Modified_On)" disabled>
                    <input type="submit" style="border-radius:5px;width:70px;height:25px;padding:0.5px;" value="Delete" />
                </form>
            </div>
        </div>
        <div class="clearfix"></div>
    </div>
</div>

<script src="~/js/lightcase.js"></script>
<script src="~/js/jquery.events.touch.js"></script>
<script>
    $('.showcase').lightcase();
</script>
<script type="text/javascript" src="~/js/move-top.js"></script>
<script type="text/javascript" src="~/js/easing.js"></script>
<script type="text/javascript">
    jQuery(document).ready(function ($) {
        $(".scroll").click(function (event) {
            event.preventDefault();
            $('html,body').animate({ scrollTop: $(this.hash).offset().top }, 1000);
        });
    });
</script>
<script type="text/javascript">
    $(document).ready(function () {
        /*
			var defaults = {
			containerID: 'toTop', // fading element id
			containerHoverID: 'toTopHover', // fading element hover id
			scrollSpeed: 1200,
			easingType: 'linear'
			};
		*/

        $().UItoTop({ easingType: 'easeOutQuart' });

    });
</script>